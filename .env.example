# LibreChat Docker installation path (where docker-compose.yml is located)
LIBRECHAT_DOCKER_PATH=/path/to/your/LibreChat

# MongoDB Configuration
MONGO_URI=mongodb://localhost:27017/
MONGO_DB_NAME=LibreChat

# Open WebUI Configuration
# This is the user ID from Open WebUI. Find it in your profile URL or the user table in the database.
TARGET_USER_ID=your_open_webui_user_id 
# Absolute path to the Open WebUI SQLite database file (e.g., C:\Users\user\data\webui.db)
SQLITE_DB_PATH=path/to/your/webui.db

# Output directory for generated presets
OUTPUT_DIR=./open_webui_presets
